{
    "name": "topukhan/geokit",
    "description": "A clean and extensible geocoding toolkit for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "topukhan",
            "email": "topukhan735@gmail.com"
        }
    ],
    "version": "dev-main",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/config": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Topukhan\\Geokit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Topukhan\\Geokit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Topukhan\\Geokit\\GeokitServiceProvider"
            ],
            "aliases": {
                "Geokit": "Topukhan\\Geokit\\Facades\\Geokit"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "geocoding",
        "address-search",
        "geoapify",
        "nominatim",
        "location"
    ]
}
